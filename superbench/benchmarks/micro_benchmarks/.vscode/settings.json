    //快速添加文件头部注释和函数注释
    "fileheader.configObj": {
        "createFileTime": false, //设置为true则为文件新建时候作为date，否则注释生成时间为date
        "autoAdd": false, //自动生成注释
        "annotationStr": {
            "head": "/*",
            "middle": " * @",
            "end": " */",
            "use": true //设置自定义注释可用
        },
        "openFunctionParamsCheck": true
    },
    "fileheader.cursorMode": {
        "description": "",
        "params ": "",
        "return": ""
    },
    "fileheader.customMade": {
        "Description": "", //文件内容描述
        "Author": "liutq", //编辑人
        "Date": "Do not edit", //时间
        "LastEditTime": "Do not edit",
        "LastEditors": "liutq",
        "Reference": ""
    },
}